                  description="Especifica la instrucción SQL utilizada por el componente para extraer datos."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
    s.OrderNumber,
    CAST(FORMAT(s.OrderDate, 'yyyyMMdd') AS INT) AS OrderDateKey,
    CAST(FORMAT(s.ShipDate, 'yyyyMMdd') AS INT) AS ShipDateKey,
    s.CustomerStateID,
    s.ProductID,
    s.Quantity,
    s.UnitPrice,
    s.DiscountAmount,
    CASE 
        WHEN s.PromotionCode IS NULL THEN 'Sin promoción'
        ELSE s.PromotionCode
    END AS PromotionCode
FROM Sales s;</property>
